{
  // 如果您是通过uni_modules形式引入uView，可以忽略此配置
  "easycom": {
    "^u-(.*)": "@/uni_modules/uview-ui/components/u-$1/u-$1.vue"
  },

  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages

    {
      "path": "pages/index/index/index",
      "style": {
        "navigationBarTitleText": "略懂工具箱",
        "enablePullDownRefresh": false
      }
    }

    , {
      "path": "pages/worklist/worklist",
      "style": {
        "navigationBarTitleText": "工作详细数据",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/worklist/work",
      "style": {
        "navigationBarTitleText": "工作分析",
        "enablePullDownRefresh": false
      }

    },
    {
      "path": "pages/worklist/listw",
      "style": {
        "navigationBarTitleText": "工作排行榜",
        "navigationStyle": "default",
        "enablePullDownRefresh": true
      }

    }

    , {
      "path": "pages/taiji/taiji",
      "style": {
        "navigationBarTitleText": "太极八卦图",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/jdurl/jdurl",
      "style": {
        "navigationBarTitleText": "京东商品查询",
        "enablePullDownRefresh": false
      }

    }, {
      "path": "pages/teamilk/teamilk",
      "style": {
        "navigationBarTitleText": "每天来一杯",
        "enablePullDownRefresh": false
      }

    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "uni-app",
    "navigationBarBackgroundColor": "#F8F8F8",
    "backgroundColor": "#F8F8F8"
  },
  "uniIdRouter": {}
}
