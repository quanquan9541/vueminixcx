// 做个表放参数
{
  "bsonType": "object",
  "required": [],
  "permission": {
    "read": false,
    "create": false,
    "update": false,
    "delete": false
  },
  "properties": {
    "_id": {
      "description": "ID，系统自动生成"
    },
    "appid": {
      "bsonType": "string",
      "title": "接口",
      "description": "接口平台"
    },
    "app_secret": {
      "bsonType": "string",
      "title": "标签",
      "description": "app_secret"
    },
    "app_key": {
      "bsonType": "string",
      "title": "密钥",
      "description": "app_key"
    },
    "app_p": {
      "bsonType": "object",
      "title": "其他"
    },
    "create_date": {
      "bsonType": "timestamp",
      "description": "创建时间",
      "forceDefaultValue": {
        "$env": "now"
      }
    }
  }
}