.jdurlbox {
  padding: 20rpx;
  margin: 20rpx;
}
.jdurlbox .history {
  margin: 400rpx 0;
}
.jdurlbox .row {
  margin: 10rpx;
}
.jdurlbox .input {
  margin: 10rpx;
}
.jdurlbox .text .title {
  margin: 20rpx 0;
  font-family: '华文细黑';
  font-style: normal;
  font-weight: bold;
  font-size: 14px;
  color: #7c90a8;
  text-overflow: clip;
  text-align: left;
  word-wrap: normal;
  text-shadow: 0px 1px 1px #c4c4c4;
}
.jdurlbox .text .content {
  margin: 20rpx 0;
  font-family: '华文细黑';
  font-style: normal;
  font-weight: normal;
  font-size: 9px;
  color: #a39aad;
  white-space: pre-line;
  text-overflow: ellipsis;
  text-align: center;
  word-wrap: break-word;
  text-shadow: 0px 1px 1px #c4c4c4;
}

